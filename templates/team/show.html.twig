{% extends 'base.html.twig' %}
{% block title %}Team{% endblock %}
{% block body %}
<div class="container site-grey-background" style="border-top:5px solid green; margin-top:15px;">
    <h1>Team</h1>
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ team.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ team.type }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('team_index') }}">back to list</a>
    <a href="{{ path('team_edit', {'id': team.id}) }}">edit</a>
    {{ include('team/_delete_form.html.twig') }}
</div>
{% endblock %}